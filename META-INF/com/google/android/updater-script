run_program("/tmp/install/bin/backuptool.sh", "restore");
set_progress(0.6);
#################################################################
# KERNEL SELECTION AROMA BEGINS
if
file_getprop("/tmp/aroma-data/kernel.prop","selected.0") == "1"
then
ui_print("");
ui_print("Installing Alucard Kernel");
package_extract_dir("jdc/kfix", "/tmp");
package_extract_file("boot.img", "/tmp/boot.img");

set_perm(0, 0, 0777, "/tmp/loki_bootloaders");
set_perm(0, 0, 0777, "/tmp/flash_kernel.sh");
set_perm(0, 0, 0777, "/tmp/loki_patch");
set_perm(0, 0, 0777, "/tmp/loki_flash");
set_perm(0, 0, 0777, "/tmp/loki.sh");

assert(run_program("/sbin/sh", "/system/etc/loki.sh") == 0);
assert(run_program("/sbin/sh", "/tmp/loki.sh")==0);
ui_print("");
endif;

if
file_getprop("/tmp/aroma-data/kernel.prop","selected.0") == "2"
then
ui_print("");
ui_print("Cleaning STweaks since stock kernel is selected...");
delete_recursive("/system/priv-app/STweaks");
delete_recursive("/data/.alucard");
delete_recursive("/data/.b--b");
ui_print("Installing Stock CM Kernel");
package_extract_dir("jdc/kfix", "/tmp");
package_extract_file("jdc/kernel/stock/boot.img", "/tmp/boot.img");
set_perm(0, 0, 0777, "/tmp/loki_bootloaders");
set_perm(0, 0, 0777, "/tmp/flash_kernel.sh");
set_perm(0, 0, 0777, "/tmp/loki_patch");
set_perm(0, 0, 0777, "/tmp/loki_flash");

run_program("/tmp/flash_kernel.sh");
endif;
set_progress(0.6);
ui_print(" ");

####################################################################
#Kernel editor

#1=stweaks
#2=kernel adiutor
#3=both stweaks and kernel adiutor
#4=none
#5=dirty install, nothing

if
file_getprop("/tmp/aroma-data/mods.prop","selected.3") == "1"
then
ui_print("STweaks will be installed");
ui_print("Removing Kernel Adiutor if exist");
delete_recursive("/system/app/KernelAdiutor");
endif;

if
file_getprop("/tmp/aroma-data/mods.prop","selected.3") == "2"
then
ui_print("Kernel adiutor will be installed");
package_extract_dir("jdc/kernelEditor/adiutor","/system/app/KernelAdiutor");
ui_print("Removing STweaks");
delete_recursive("/system/priv-app/STweaks");
delete_recursive("/data/.alucard");
delete_recursive("/data/.b--b");
endif;

if
file_getprop("/tmp/aroma-data/mods.prop","selected.3") == "3"
then
ui_print("STweaks and Kernel Adiutor will be installed");
package_extract_dir("jdc/kernelEditor/adiutor","/system/app/KernelAdiutor");
endif;

if
file_getprop("/tmp/aroma-data/mods.prop","selected.3") == "4"
then
ui_print("Removing STweaks if exist");
delete_recursive("/system/priv-app/STweaks");
delete_recursive("/data/.alucard");
ui_print("Removing Kernel Adiutor if exist");
delete_recursive("/system/app/KernelAdiutor");
endif;

if
file_getprop("/tmp/aroma-data/mods.prop","selected.3") == "5"
then
ui_print("Keeping the old kernel editor if exists.");
endif;

######################################end of kernel editor

set_progress(0.9);

#Sound tweaks
if
file_getprop("/tmp/aroma-data/mods.prop","selected.2") == "1"
then
ui_print("Installing BusyBox which is required for ViPER4Arise");
package_extract_dir("system/busybox", "/tmp/busybox");
run_program("/sbin/busybox", "unzip", "/tmp/busybox/bb.zip", "META-INF/com/google/android/update-binary", "-d", "/tmp/busybox");
run_program("/sbin/busybox", "sh", "/tmp/busybox/META-INF/com/google/android/update-binary", "null", "1", "/tmp/busybox/bb.zip");

ui_print("Installing ViPER4Arise");
package_extract_dir("system/arise", "/tmp/arise");
run_program("/sbin/busybox", "unzip", "/tmp/arise/arise.zip", "META-INF/com/google/android/update-binary", "-d", "/tmp/arise");
run_program("/sbin/busybox", "sh", "/tmp/arise/META-INF/com/google/android/update-binary", "null", "1", "/tmp/arise/arise.zip");

ui_print("Cleaning temp files...");
delete_recursive("/tmp/busybox");
delete_recursive("/tmp/aries");
ui_print("ViPER4Arise installed successfully");
ui_print("");
endif;

#Camera options
#################################
if
file_getprop("/tmp/aroma-data/mods.prop","selected.4") == "1"
then
ui_print("AOSP Camera selected");
endif;

if
file_getprop("/tmp/aroma-data/mods.prop","selected.4") == "2"
then
delete_recursive("/system/app/Camera2");
ui_print("AOSP Camera will be removed.Snap Camera will be installed");
package_extract_dir("jdc/camera/Snap","/system/app/Snap");
set_perm(0, 0, 0644, "/system/priv-app/Snap/Snap.apk");
set_perm(0, 0, 0644, "/system/priv-app/Snap/oat/arm/Snap.odex");
ui_print("");
endif;

if
file_getprop("/tmp/aroma-data/mods.prop","selected.4") == "3"
then
ui_print("Installing both AOSP and Snap Camera");
package_extract_dir("jdc/camera/Snap","/system/app/Snap");
set_perm(0, 0, 0644, "/system/priv-app/Snap/Snap.apk");
set_perm(0, 0, 0644, "/system/priv-app/Snap/oat/arm/Snap.odex");
ui_print("");
endif;


#################################end of camera options

ui_print(" ");
ui_print("Installation complete");
ui_print("Unmounting partitions...");
unmount("/data");
unmount("/cache");
unmount("/preload");
unmount("/system");

#SuperSU installation IF exist
if
file_getprop("/tmp/aroma-data/mods.prop","selected.1") == "1"
then
	ui_print("Installing SuperSU...");
	package_extract_dir("system/supersu", "/tmp/supersu");
	run_program("/sbin/busybox", "unzip", "/tmp/supersu/supersu.zip", "META-INF/com/google/android/update-binary", "-d", "/tmp/supersu");
	run_program("/sbin/busybox", "sh", "/tmp/supersu/META-INF/com/google/android/update-binary", "null", "1", "/tmp/supersu/supersu.zip");
	delete_recursive("/tmp/supersu");
	delete_recursive("/system/supersu");
	ui_print("SuperSU installed successfully");
	ui_print("Reboot may cause a bootloop.Its due to superSU installation.");
endif;

#Privacy Guard
if
file_getprop("/tmp/aroma-data/mods.prop","selected.1") == "2"
then
ui_print("Privacy guard will be installed");
endif;

ui_print("___________________________________________");
ui_print("Done!");
set_progress(1.0);
